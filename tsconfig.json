{
  "compilerOptions": {
    "target": "es2020",
    "module": "commonjs",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "rootDir": "./src",
    "outDir": "./dist",
    "strict": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "strict": false,
    "skipLibCheck": true

  },
 "include": ["src/**/*.ts", "src/types.d.ts"],
  "exclude": ["node_modules", "dist"]
}
